import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner scan = new Scanner(System.in);
        int amount = scan.nextInt();
        scan.nextLine();
        String noteStatus = scan.nextLine();
        
        if(noteStatus.equals("available") && (amount %100 == 0) || noteStatus.equals("not available") && (amount % 500 == 0)){
                System.out.printf("Amount %d can be withdrawn",amount);
        }
        else{
            System.out.println("Enter amount in multiples of 500");
        }
    }
}
